@model List<Entidades.Jugador>
    @{
        Layout = "~/Views/Shared/_Layout.cshtml";
    }


    <h1 class="p-5">Jugadores sin club</h1>

    <table>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
        </tr>
        @foreach (Entidades.Jugador jug in Model)
        {
            <tr>
                <td>@jug.Nombre</td>
                <td>@jug.Apellido</td>
            </tr>
        }

    </table>
